	var qInvParam='';
	//END CUSTOM CODE
	function a(param, _value){
		qInvParam += '&'+param+'='+ _value;
	}
	function readCookie(name)
	{
  	var ca = document.cookie.split(';');
  	var nameEQ = name + "=";
  	for(var i=0; i < ca.length; i++) {
    	var c = ca[i];
    	while (c.charAt(0)==' ') c = c.substring(1, c.length); //delete spaces
    	if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length, c.length);
    }
  	return null;
	}

	function fSubmit() {
		today=new Date();
		endsek=today.getSeconds();
		endmin=today.getMinutes();
		endtim=today.getHours();
		enda=(endsek) + 60 * (endmin) + 3600 * (endtim); 
		totalTime=enda-start_Time;
		a('totalTime', totalTime); a('startTime', start_Time); a('endTime', enda); 
		var _srCvals = unescape(readCookie("msresearch")).replace(/\":\"|\":/gi, "=");
		var _srCvals = _srCvals.replace(/\",\"|\":{\"|,\"|{\"/gi, "&");		
		var _srCvals = _srCvals.replace(/}|\[|\]/gi, "");
		pa_args = pa_args;	
		var _params =  pa_args + qInvParam + _srCvals;
		//_params = _params.replace(/referrer=/gi, "referrer="+_getref);
		//var sURL = "&sUrl=http://survey2.surveysite.com/wix/p178132240.aspx&cookiePid=p179687135";
		//("params to be passed: " + _params);
		var url = "http://survey2.surveysite.com/wix/p127227702.aspx?"+_params;
		window.location.href = url;
	}
